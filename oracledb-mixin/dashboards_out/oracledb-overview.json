         {
            "description": "Database status either Up or Down. Colored to be green when Up or red when Down",
            "fieldConfig": {
               "defaults": {
                  "color": {
                     "mode": "thresholds"
                  },
                  "mappings": [
                     {
                        "options": {
                           "0": {
                              "index": 0,
                              "text": "Down"
                           },
                           "1": {
                              "index": 1,
                              "text": "OK"
                           }
                        },
                        "type": "value"
                     }
                  ],
                  "thresholds": {
                     "mode": "absolute",
                     "steps": [
                        {
                           "color": "red"
                        },
                        {
                           "color": "green",
                           "value": 1
