                  "expr": "oracledb_up{job=~\"integrations/oracledb\",job=~\"$job\",instance=~\"$instance\"}",
                  "format": "time_series",
                  "instant": false,
                  "legendFormat": "{{instance}}",
                  "refId": "Database status"
