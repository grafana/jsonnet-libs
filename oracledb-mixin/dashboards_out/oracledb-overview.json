            "datasource": {
               "type": "datasource",
               "uid": "-- Mixed --"
            },
            "description": "Database status either Up or Down. Colored to be green when Up or red when Down.",
            "fieldConfig": {
               "defaults": {
                  "mappings": [
                     {
                        "options": {
                           "0": {
                              "index": 0,
                              "text": "Down"
                           },
                           "1": {
                              "index": 1,
                              "text": "OK"
                           }
                        },
                        "type": "value"
                     }
                  ],
                  "thresholds": {
                     "steps": [
                        {
                           "color": "red"
                        },
                        {
                           "color": "green",
                           "value": 1
                        }
                     ]
                  }
               }
            },
            "gridPos": {
               "h": 8,
               "w": 4,
               "x": 0,
               "y": 1
