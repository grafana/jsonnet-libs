                  "expr": "sum by(job,hbase_cluster) (master_num_calls_in_replication_queue{job=\"integrations/apache-hbase\",job=~\"$job\",hbase_cluster=~\"$hbase_cluster\",instance=~\"$instance\"})",
                  "format": "time_series",
                  "instant": false,
                  "legendFormat": "{{hbase_cluster}} - replication",
                  "refId": "Master calls in replication queue"
               },
               {
                  "datasource": {
                     "type": "prometheus",
                     "uid": "${prometheus_datasource}"
                  },
                  "expr": "sum by(job,hbase_cluster) (master_num_calls_in_read_queue{job=\"integrations/apache-hbase\",job=~\"$job\",hbase_cluster=~\"$hbase_cluster\",instance=~\"$instance\"})",
                  "format": "time_series",
                  "instant": false,
                  "legendFormat": "{{hbase_cluster}} - read",
