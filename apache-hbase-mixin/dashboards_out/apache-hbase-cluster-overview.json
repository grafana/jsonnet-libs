               {
                  "datasource": {
                     "type": "prometheus",
                     "uid": "${prometheus_datasource}"
                  },
                  "expr": "sum by(job,hbase_cluster) (master_num_calls_in_write_queue{job=\"integrations/apache-hbase\",job=~\"$job\",hbase_cluster=~\"$hbase_cluster\",instance=~\"$instance\"})",
                  "format": "time_series",
                  "instant": false,
                  "legendFormat": "{{hbase_cluster}} - write",
                  "refId": "Master calls in write queue"
               },
               {
                  "datasource": {
                     "type": "prometheus",
                     "uid": "${prometheus_datasource}"
                  },
                  "expr": "sum by(job,hbase_cluster) (master_num_calls_in_scan_queue{job=\"integrations/apache-hbase\",job=~\"$job\",hbase_cluster=~\"$hbase_cluster\",instance=~\"$instance\"})",
                  "format": "time_series",
                  "instant": false,
                  "legendFormat": "{{hbase_cluster}} - scan",
                  "refId": "Master calls in scan queue"
               },
               {
                  "datasource": {
                     "type": "prometheus",
                     "uid": "${prometheus_datasource}"
                  },
                  "expr": "sum by(job,hbase_cluster) (master_num_calls_in_priority_queue{job=\"integrations/apache-hbase\",job=~\"$job\",hbase_cluster=~\"$hbase_cluster\",instance=~\"$instance\"})",
                  "format": "time_series",
                  "instant": false,
                  "legendFormat": "{{hbase_cluster}} - priority",
                  "refId": "Master calls in priority queue"
               }
