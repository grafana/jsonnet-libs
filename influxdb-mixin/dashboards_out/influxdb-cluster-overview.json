                  "expr": "rate(boltdb_reads_total{job=\"integrations/influxdb\",job=~\"$job\",influxdb_cluster=~\"$influxdb_cluster\",instance=~\"$instance\"}[$__rate_interval])",
                  "format": "time_series",
                  "instant": false,
                  "interval": "1m",
                  "legendFormat": "{{influxdb_cluster}} - read",
