         {
            "datasource": {
               "type": "datasource",
               "uid": "-- Mixed --"
            },
            "description": "Network traffic (bits per sec) measures data transmitted and received.",
            "fieldConfig": {
               "defaults": {
                  "custom": {
                     "axisCenteredZero": false,
                     "axisLabel": "out(-) | in(+)",
                     "fillOpacity": 30,
                     "gradientMode": "opacity",
                     "lineInterpolation": "smooth",
                     "lineWidth": 2,
                     "showPoints": "never",
                     "stacking": {
                        "mode": "normal"
                     }
                  },
                  "decimals": 1,
                  "unit": "bps"
               },
               "overrides": [
                  {
                     "matcher": {
                        "id": "byRegexp",
                        "options": "/transmit|tx|out/"
                     },
                     "properties": [
                        {
                           "id": "custom.transform",
                           "value": "negative-Y"
                        }
                     ]
                  },
                  {
                     "matcher": {
                        "id": "byFrameRefID",
                        "options": "received"
                     },
                     "properties": [
                        {
                           "id": "unit",
                           "value": "bps"
                        }
                     ]
                  },
                  {
                     "matcher": {
                        "id": "byFrameRefID",
                        "options": "transmitted"
                     },
                     "properties": [
                        {
                           "id": "unit",
                           "value": "bps"
                        }
                     ]
