         {
            "datasource": {
               "type": "prometheus",
               "uid": "${datasource}"
            },
            "description": "Current memory usage in bytes, including all memory regardless of when it was accessed by container.",
            "fieldConfig": {
               "defaults": {
                  "custom": {
                     "fillOpacity": 30,
                     "gradientMode": "opacity",
                     "lineInterpolation": "smooth",
                     "lineWidth": 2,
                     "showPoints": "never",
                     "stacking": {
                        "mode": "normal"
                     }
                  },
                  "min": 0,
                  "unit": "bytes"
               },
               "overrides": [
                  {
                     "matcher": {
                        "id": "byFrameRefID",
                        "options": "Memory usage"
                     },
                     "properties": [
                        {
                           "id": "unit",
                           "value": "decbytes"
                        }
                     ]
                  },
                  {
                     "matcher": {
                        "id": "byRegexp",
                        "options": ".*(T|t)otal.*"
