         {
            "keepTime": true,
            "title": "SAP HANA logs",
            "type": "link",
            "url": "/d/saphana_logs"
         },
         {
            "asDropdown": true,
            "includeVars": true,
            "keepTime": true,
            "tags": [
               "sap-hana-mixin"
            ],
            "title": "All dashboards",
            "type": "dashboards"
         },
         {
            "keepTime": true,
            "title": "SAP HANA instance overview",
            "type": "link",
            "url": "/d/saphana_instance_overview"
