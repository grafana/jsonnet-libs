                  "refId": "Insert queries"
               },
               {
                  "datasource": {
                     "type": "prometheus",
                     "uid": "${prometheus_datasource}"
                  },
                  "expr": "rate(ClickHouseProfileEvents_AsyncInsertQuery{job=~\"integrations/clickhouse\",job=~\"$job\",instance=~\"$instance\"}[$__rate_interval])",
                  "format": "time_series",
                  "instant": false,
