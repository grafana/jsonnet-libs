         {
            "datasource": {
               "type": "prometheus",
               "uid": "${prometheus_datasource}"
            },
            "description": "Percentage of memory allocated by ClickHouse compared to OS total",
            "fieldConfig": {
               "defaults": {
                  "thresholds": {
                     "steps": [
                        {
                           "color": "super-light-green",
                           "value": 0
                        },
                        {
                           "color": "#EAB839",
                           "value": 80
                        },
                        {
                           "color": "super-light-red",
                           "value": 90
                        }
                     ]
                  },
                  "unit": "percent"
               }
            },
            "gridPos": {
               "h": 8,
               "w": 12,
               "x": 12,
               "y": 16
            },
            "id": 5,
            "options": {
               "showThresholdLabels": true
            },
            "pluginVersion": "v11.0.0",
            "targets": [
               {
                  "datasource": {
                     "type": "prometheus",
                     "uid": "${prometheus_datasource}"
                  },
                  "expr": "(ClickHouseMetrics_MemoryTracking{job=~\"integrations/clickhouse\",job=~\"$job\",instance=~\"$instance\"} / ClickHouseAsyncMetrics_OSMemoryTotal{job=~\"integrations/clickhouse\",job=~\"$job\",instance=~\"$instance\"}) * 100",
                  "format": "time_series",
                  "instant": false,
                  "legendFormat": "Memory tracking percent",
                  "refId": "Memory usage percentage"
               }
            ],
            "title": "Memory usage",
            "type": "gauge"
