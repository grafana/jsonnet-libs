{
      "annotations": {
         "list": [ ]
      },
      "editable": false,
      "id": null,
      "links": [
         {
            "keepTime": true,
            "title": "GitLab logs",
            "type": "link",
            "url": "/d/gitlab-logs"
         },
         {
            "asDropdown": true,
            "includeVars": true,
            "keepTime": true,
            "tags": [
               "gitlab"
            ],
            "title": "All dashboards",
            "type": "dashboards"
         }
      ],
      "panels": [
         {
            "collapsed": false,
            "gridPos": {
               "h": 1,
               "w": 0,
               "x": 0,
               "y": 0
            },
            "id": 1,
            "panels": [ ],
            "title": "Overview",
            "type": "row"
         },
         {
            "datasource": {
               "type": "prometheus",
               "uid": "${prometheus_datasource}"
            },
            "description": "The rate of user logins.",
            "fieldConfig": {
               "defaults": {
                  "custom": {
                     "fillOpacity": 30,
                     "gradientMode": "opacity",
                     "lineInterpolation": "smooth",
                     "lineWidth": 2,
                     "showPoints": "never",
                     "spanNulls": false
                  },
                  "unit": "sessions/s"
               }
            },
            "gridPos": {
               "h": 8,
               "w": 8,
               "x": 0,
               "y": 1
            },
            "id": 2,
            "options": {
               "legend": {
                  "calcs": [ ],
                  "displayMode": "list"
               },
               "tooltip": {
                  "mode": "multi",
                  "sort": "desc"
               }
            },
            "pluginVersion": "v11.0.0",
            "targets": [
               {
                  "datasource": {
                     "type": "prometheus",
                     "uid": "${prometheus_datasource}"
                  },
                  "expr": "rate(user_session_logins_total{job=\"integrations/gitlab\",job=~\"$job\",cluster=~\"$cluster\",instance=~\"$instance\"}[$__rate_interval])",
                  "format": "time_series",
                  "instant": false,
                  "interval": "1m",
                  "legendFormat": "sessions",
                  "refId": "User sessions"
               }
            ],
            "title": "User Sessions",
            "type": "timeseries"
         },
         {
            "datasource": {
               "type": "prometheus",
               "uid": "${prometheus_datasource}"
            },
            "description": "Average latency of inbound HTTP requests.",
            "fieldConfig": {
               "defaults": {
                  "custom": {
                     "spanNulls": false
                  },
                  "unit": "ms"
               }
            },
            "gridPos": {
               "h": 8,
               "w": 8,
               "x": 8,
               "y": 1
            },
            "id": 3,
            "pluginVersion": "v11.0.0",
            "targets": [
               {
                  "datasource": {
                     "type": "prometheus",
                     "uid": "${prometheus_datasource}"
                  },
                  "expr": "1000 * rate(http_request_duration_seconds_sum{job=\"integrations/gitlab\",job=~\"$job\",cluster=~\"$cluster\",instance=~\"$instance\"}[$__rate_interval]) / rate(http_request_duration_seconds_count{job=\"integrations/gitlab\",job=~\"$job\",cluster=~\"$cluster\",instance=~\"$instance\"}[$__rate_interval])",
                  "format": "time_series",
                  "instant": false,
                  "interval": "1m",
                  "legendFormat": "{{ method }}",
                  "refId": "Average request latency"
               }
            ],
            "title": "Average Request Latency",
            "type": "timeseries"
         },
         {
            "datasource": {
               "type": "prometheus",
               "uid": "${prometheus_datasource}"
            },
            "description": "Top 5 types of requests to the server.",
            "fieldConfig": {
               "defaults": {
                  "unit": "reqps"
               }
            },
            "gridPos": {
               "h": 8,
               "w": 8,
               "x": 16,
               "y": 1
            },
            "id": 4,
            "pluginVersion": "v11.0.0",
            "targets": [
               {
                  "datasource": {
                     "type": "prometheus",
                     "uid": "${prometheus_datasource}"
                  },
                  "expr": "sum by (feature_category) (topk(5, rate(http_requests_total{job=\"integrations/gitlab\",job=~\"$job\",cluster=~\"$cluster\",instance=~\"$instance\"}[$__rate_interval])))",
                  "format": "table",
                  "instant": true,
                  "interval": "2m",
                  "legendFormat": "{{instance}}: Top request types",
                  "refId": "Top request types"
               }
            ],
            "title": "Top 5 Request Types",
            "transformations": [
               {
                  "id": "groupBy",
                  "options": {
                     "fields": {
                        "Value": {
                           "aggregations": [
                              "lastNotNull"
                           ],
                           "operation": "aggregate"
                        },
                        "feature_category": {
                           "aggregations": [
                              "lastNotNull"
                           ],
                           "operation": "groupby"
                        }
                     }
                  }
               },
               {
                  "id": "organize",
                  "options": {
                     "excludeByName": { },
                     "indexByName": { },
                     "renameByName": {
                        "Value (lastNotNull)": "Request rate",
                        "feature_category": "Feature category"
                     }
                  }
               },
               {
                  "id": "sortBy",
                  "options": {
                     "fields": { },
                     "sort": [
                        {
                           "desc": true,
                           "field": "Request rate"
                        }
                     ]
                  }
               },
               {
                  "id": "limit",
                  "options": {
                     "limitField": 5
                  }
               }
            ],
            "type": "table"
         },
         {
            "datasource": {
               "type": "prometheus",
               "uid": "${prometheus_datasource}"
            },
            "description": "Rate of HTTP traffic over time, grouped by status.",
            "fieldConfig": {
               "defaults": {
                  "custom": {
                     "fillOpacity": 30,
                     "gradientMode": "opacity",
                     "lineInterpolation": "smooth",
                     "lineWidth": 2,
                     "showPoints": "never",
                     "spanNulls": false
                  },
                  "unit": "reqps"
               }
            },
            "gridPos": {
               "h": 8,
               "w": 24,
               "x": 0,
               "y": 9
            },
            "id": 5,
            "options": {
               "legend": {
                  "calcs": [ ],
                  "displayMode": "list"
               },
               "tooltip": {
                  "mode": "multi",
                  "sort": "desc"
               }
            },
            "pluginVersion": "v11.0.0",
            "targets": [
               {
                  "datasource": {
                     "type": "prometheus",
                     "uid": "${prometheus_datasource}"
                  },
                  "expr": "sum by (status) (rate(http_requests_total{job=\"integrations/gitlab\",job=~\"$job\",cluster=~\"$cluster\",instance=~\"$instance\"}[$__rate_interval]))",
                  "format": "time_series",
                  "instant": false,
                  "interval": "1m",
                  "legendFormat": "{{ status }}",
                  "refId": "Traffic by Response Code"
               }
            ],
            "title": "Traffic by Response Code",
            "type": "timeseries"
         },
         {
            "collapsed": false,
            "gridPos": {
               "h": 1,
               "w": 0,
               "x": 0,
               "y": 17
            },
            "id": 6,
            "panels": [ ],
            "title": "Pipeline Activity",
            "type": "row"
         },
         {
            "datasource": {
               "type": "prometheus",
               "uid": "${prometheus_datasource}"
            },
            "description": "The number of jobs activated per second.",
            "fieldConfig": {
               "defaults": {
                  "custom": {
                     "fillOpacity": 30,
                     "gradientMode": "opacity",
                     "lineInterpolation": "smooth",
                     "lineWidth": 2,
                     "showPoints": "never",
                     "spanNulls": false
                  },
                  "unit": "activations/s"
               }
            },
            "gridPos": {
               "h": 8,
               "w": 24,
               "x": 0,
               "y": 18
            },
            "id": 7,
            "options": {
               "legend": {
                  "calcs": [ ],
                  "displayMode": "list"
               },
               "tooltip": {
                  "mode": "multi",
                  "sort": "desc"
               }
            },
            "pluginVersion": "v11.0.0",
            "targets": [
               {
                  "datasource": {
                     "type": "prometheus",
                     "uid": "${prometheus_datasource}"
                  },
                  "expr": "rate(gitlab_ci_active_jobs_sum{job=\"integrations/gitlab\",job=~\"$job\",cluster=~\"$cluster\",instance=~\"$instance\"}[$__rate_interval])",
                  "format": "time_series",
                  "instant": false,
                  "interval": "1m",
                  "legendFormat": "{{ plan }}",
                  "refId": "GitLab CI active jobs"
               }
            ],
            "title": "Job Activations",
            "type": "timeseries"
         },
         {
            "datasource": {
               "type": "prometheus",
               "uid": "${prometheus_datasource}"
            },
            "description": "Rate of pipeline instances created.",
            "fieldConfig": {
               "defaults": {
                  "custom": {
                     "fillOpacity": 30,
                     "gradientMode": "opacity",
                     "lineInterpolation": "smooth",
                     "lineWidth": 2,
                     "showPoints": "never",
                     "spanNulls": false
                  },
                  "unit": "pipelines/s"
               }
            },
            "gridPos": {
               "h": 8,
               "w": 12,
               "x": 0,
               "y": 26
            },
            "id": 8,
            "options": {
               "legend": {
                  "calcs": [ ],
                  "displayMode": "list"
               },
               "tooltip": {
                  "mode": "multi",
                  "sort": "desc"
               }
            },
            "pluginVersion": "v11.0.0",
            "targets": [
               {
                  "datasource": {
                     "type": "prometheus",
                     "uid": "${prometheus_datasource}"
                  },
                  "expr": "rate(pipelines_created_total{job=\"integrations/gitlab\",job=~\"$job\",cluster=~\"$cluster\",instance=~\"$instance\"}[$__rate_interval])",
                  "format": "time_series",
                  "instant": false,
                  "interval": "1m",
                  "legendFormat": "{{ source }}",
                  "refId": "Pipelines created"
               }
            ],
            "title": "Pipelines Created",
            "type": "timeseries"
         },
         {
            "datasource": {
               "type": "prometheus",
               "uid": "${prometheus_datasource}"
            },
            "description": "The number of builds created within a pipeline per second, grouped by source.",
            "fieldConfig": {
               "defaults": {
                  "custom": {
                     "fillOpacity": 30,
                     "gradientMode": "opacity",
                     "lineInterpolation": "smooth",
                     "lineWidth": 2,
                     "showPoints": "never",
                     "spanNulls": false
                  },
                  "unit": "builds/s"
               }
            },
            "gridPos": {
               "h": 8,
               "w": 12,
               "x": 12,
               "y": 26
            },
            "id": 9,
            "options": {
               "legend": {
                  "calcs": [ ],
                  "displayMode": "list"
               },
               "tooltip": {
                  "mode": "multi",
                  "sort": "desc"
               }
            },
            "pluginVersion": "v11.0.0",
            "targets": [
               {
                  "datasource": {
                     "type": "prometheus",
                     "uid": "${prometheus_datasource}"
                  },
                  "expr": "sum by (source) (rate(gitlab_ci_pipeline_size_builds_sum{job=\"integrations/gitlab\",job=~\"$job\",cluster=~\"$cluster\",instance=~\"$instance\"}[$__rate_interval]))",
                  "format": "time_series",
                  "instant": false,
                  "interval": "1m",
                  "legendFormat": "{{ source }}",
                  "refId": "Pipeline builds created"
               }
            ],
            "title": "Pipeline Builds Created",
            "type": "timeseries"
         },
         {
            "datasource": {
               "type": "prometheus",
               "uid": "${prometheus_datasource}"
            },
            "description": "The rate of build trace operations performed, grouped by source.",
            "fieldConfig": {
               "defaults": {
                  "custom": {
                     "fillOpacity": 30,
                     "gradientMode": "opacity",
                     "lineInterpolation": "smooth",
                     "lineWidth": 2,
                     "showPoints": "never",
                     "spanNulls": false
                  },
                  "unit": "ops"
               }
            },
            "gridPos": {
               "h": 8,
               "w": 24,
               "x": 0,
               "y": 34
            },
            "id": 10,
            "options": {
               "legend": {
                  "calcs": [ ],
                  "displayMode": "list"
               },
               "tooltip": {
                  "mode": "multi",
                  "sort": "desc"
               }
            },
            "pluginVersion": "v11.0.0",
            "targets": [
               {
                  "datasource": {
                     "type": "prometheus",
                     "uid": "${prometheus_datasource}"
                  },
                  "expr": "rate(gitlab_ci_trace_operations_total{job=\"integrations/gitlab\",job=~\"$job\",cluster=~\"$cluster\",instance=~\"$instance\"}[$__rate_interval])",
                  "format": "time_series",
                  "instant": false,
                  "interval": "1m",
                  "legendFormat": "{{ operation }}",
                  "refId": "Build trace operations"
               }
            ],
            "title": "Build Trace Operations",
            "type": "timeseries"
         }
      ],
      "refresh": "30s",
      "schemaVersion": 39,
      "tags": [
         "gitlab"
      ],
      "templating": {
         "list": [
            {
               "label": "Prometheus data source",
               "name": "prometheus_datasource",
               "query": "prometheus",
               "regex": "(?!grafanacloud-usage|grafanacloud-ml-metrics).+",
               "type": "datasource"
            },
            {
               "allValue": ".+",
               "datasource": {
                  "type": "prometheus",
                  "uid": "${prometheus_datasource}"
               },
               "includeAll": true,
               "label": "Job",
               "multi": true,
               "name": "job",
               "query": "label_values(gitlab_cache_misses_total{job=\"integrations/gitlab\"}, job)",
               "refresh": 2,
               "sort": 1,
               "type": "query"
            },
            {
               "allValue": ".*",
               "datasource": {
                  "type": "prometheus",
                  "uid": "${prometheus_datasource}"
               },
               "includeAll": true,
               "label": "Cluster",
               "multi": true,
               "name": "cluster",
               "query": "label_values(gitlab_cache_misses_total{job=\"integrations/gitlab\",job=~\"$job\"}, cluster)",
               "refresh": 2,
               "sort": 1,
               "type": "query"
            },
            {
               "allValue": ".+",
               "datasource": {
                  "type": "prometheus",
                  "uid": "${prometheus_datasource}"
               },
               "includeAll": true,
               "label": "Instance",
               "multi": true,
               "name": "instance",
               "query": "label_values(gitlab_cache_misses_total{job=\"integrations/gitlab\",job=~\"$job\",cluster=~\"$cluster\"}, instance)",
               "refresh": 2,
               "sort": 1,
               "type": "query"
            },
            {
               "hide": 2,
               "label": "Loki data source",
               "name": "loki_datasource",
               "query": "loki",
               "regex": "(?!grafanacloud.+usage-insights|grafanacloud.+alert-state-history).+",
               "type": "datasource"
            }
         ]
      },
      "time": {
         "from": "now-30m",
         "to": "now"
      },
      "timezone": "default",
      "title": "GitLab overview",
      "uid": "gitlab_overview"
   }