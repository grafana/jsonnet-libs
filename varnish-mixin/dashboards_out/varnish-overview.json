            "description": "Rate of cache hits to misses.",
            "fieldConfig": {
               "defaults": {
                  "max": 100,
                  "min": 0,
                  "thresholds": {
                     "mode": "absolute",
                     "steps": [
                        {
                           "color": "red",
                           "value": 0
                        },
                        {
                           "color": "#EAB839",
                           "value": 50
                        },
                        {
                           "color": "green",
                           "value": 80
                        }
                     ]
                  },
                  "unit": "percent"
               }
            },
            "gridPos": {
               "h": 4,
               "w": 3,
               "x": 0,
               "y": 1
