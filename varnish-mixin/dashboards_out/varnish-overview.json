            "id": 2,
            "options": {
               "orientation": "auto",
               "showThresholdLabels": false,
               "showThresholdMarkers": true
            },
            "pluginVersion": "v11.4.0",
            "targets": [
               {
                  "datasource": {
                     "type": "prometheus",
                     "uid": "${prometheus_datasource}"
                  },
                  "expr": "avg((rate(varnish_main_cache_hit{job=~\"$job\",cluster=~\"$cluster\",instance=~\"$instance\"}[$__rate_interval]) / clamp_min(rate(varnish_main_cache_hit{job=~\"$job\",cluster=~\"$cluster\",instance=~\"$instance\"}[$__rate_interval]) + rate(varnish_main_cache_miss{job=~\"$job\",cluster=~\"$cluster\",instance=~\"$instance\"}[$__rate_interval]), 1))) by () * 100",
                  "format": "time_series",
                  "instant": false,
                  "legendFormat": "{{instance}}",
                  "refId": "Cache hit rate"
               }
            ],
            "title": "Cache hit rate",
            "transparent": true,
            "type": "gauge"
         },
