            },
            "gridPos": {
               "h": 8,
               "w": 12,
               "x": 0,
               "y": 26
            },
            "id": 11,
            "options": {
               "legend": {
                  "placement": "right"
               },
               "tooltip": {
                  "mode": "multi"
               }
            },
            "pluginVersion": "v11.4.0",
            "targets": [
               {
                  "datasource": {
                     "type": "prometheus",
                     "uid": "${datasource}"
                  },
                  "expr": "increase(airflow_dagrun_schedule_delay_sum{job=~\"$job\", instance=~\"$instance\",job=~\"$job\",instance=~\"$instance\", dag_id=~\"$dag_id\"}[$__interval:])/clamp_min(increase(airflow_dagrun_schedule_delay_count{job=~\"$job\", instance=~\"$instance\",job=~\"$job\",instance=~\"$instance\", dag_id=~\"$dag_id\"}[$__interval:]),1)",
                  "format": "time_series",
                  "instant": false,
                  "interval": "1m",
                  "legendFormat": "{{instance}} - {{dag_id}}",
                  "refId": "Average DAG schedule delay"
               }
            ],
            "title": "DAG schedule delay",
            "type": "timeseries"
         },
         {
            "datasource": {
               "type": "datasource",
