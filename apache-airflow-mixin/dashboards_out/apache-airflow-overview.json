{
      "annotations": {
         "list": [ ]
      },
      "description": "Dashboard providing an overview of Apache Airflow DAGs, tasks, and scheduler performance.",
      "links": [
         {
            "keepTime": true,
            "title": "Apache Airflow logs",
            "type": "link",
            "url": "/d/apacheairflow_logs"
         },
         {
            "asDropdown": true,
            "includeVars": true,
            "keepTime": true,
            "tags": [
               "apache-airflow-mixin"
            ],
            "title": "All dashboards",
            "type": "dashboards"
         }
      ],
      "panels": [
         {
            "collapsed": false,
            "gridPos": {
               "h": 1,
               "w": 0,
               "x": 0,
               "y": 0
            },
            "id": 1,
            "panels": [ ],
            "title": "Apache Airflow overview",
            "type": "row"
         },
         {
            "datasource": {
               "type": "datasource",
               "uid": "-- Mixed --"
            },
            "description": "The number of errors from trying to parse DAG files in an Apache Airflow system.",
            "fieldConfig": {
               "defaults": {
                  "decimals": 0,
                  "min": 0,
                  "thresholds": {
                     "steps": [
                        {
                           "color": "green",
                           "value": null
                        },
                        {
                           "color": "red",
                           "value": 1
                        }
                     ]
                  },
                  "unit": "none"
               }
            },
            "gridPos": {
               "h": 8,
               "w": 8,
               "x": 0,
               "y": 1
            },
            "id": 2,
            "pluginVersion": "v11.4.0",
            "targets": [
               {
                  "datasource": {
                     "type": "prometheus",
                     "uid": "${datasource}"
                  },
                  "expr": "airflow_dag_processing_import_errors{job=~\"$job\", instance=~\"$instance\",job=~\"$job\",instance=~\"$instance\"}",
                  "format": "time_series",
                  "instant": false,
                  "legendFormat": "{{instance}}",
                  "refId": "DAG file parsing errors"
               }
            ],
            "title": "DAG file parsing errors",
            "type": "timeseries"
         },
         {
            "datasource": {
               "type": "datasource",
               "uid": "-- Mixed --"
            },
            "description": "The number of Service Level Agreement misses for any DAG runs in the Apache Airflow system.",
            "fieldConfig": {
               "defaults": {
                  "decimals": 0,
                  "min": 0,
                  "thresholds": {
                     "steps": [
                        {
                           "color": "green",
                           "value": null
                        },
                        {
                           "color": "red",
                           "value": 80
                        }
                     ]
                  },
                  "unit": "none"
               }
            },
            "gridPos": {
               "h": 8,
               "w": 8,
               "x": 8,
               "y": 1
            },
            "id": 3,
            "pluginVersion": "v11.4.0",
            "targets": [
               {
                  "datasource": {
                     "type": "prometheus",
                     "uid": "${datasource}"
                  },
                  "expr": "rate(airflow_sla_missed{job=~\"$job\", instance=~\"$instance\",job=~\"$job\",instance=~\"$instance\", dag_id=~\"$dag_id\", task_id=~\"$task_id\"}[$__rate_interval])",
                  "format": "time_series",
                  "instant": false,
                  "interval": "1m",
                  "legendFormat": "{{instance}}",
                  "refId": "SLA misses"
               }
            ],
            "title": "SLA misses",
            "type": "timeseries"
         },
         {
            "datasource": {
               "type": "datasource",
               "uid": "-- Mixed --"
            },
            "description": "The overall task instances failures for an Apache Airflow system.",
            "fieldConfig": {
               "defaults": {
                  "decimals": 0,
                  "min": 0,
                  "thresholds": {
                     "steps": [
                        {
                           "color": "green",
                           "value": null
                        },
                        {
                           "color": "red",
                           "value": 1
                        }
                     ]
                  },
                  "unit": "none"
               }
            },
            "gridPos": {
               "h": 8,
               "w": 8,
               "x": 16,
               "y": 1
            },
            "id": 4,
            "pluginVersion": "v11.4.0",
            "targets": [
               {
                  "datasource": {
                     "type": "prometheus",
                     "uid": "${datasource}"
                  },
                  "expr": "rate(airflow_ti_failures{job=~\"$job\", instance=~\"$instance\",job=~\"$job\",instance=~\"$instance\", dag_id=~\"$dag_id\", task_id=~\"$task_id\"}[$__rate_interval])",
                  "format": "time_series",
                  "instant": false,
                  "interval": "1m",
                  "legendFormat": "{{instance}}",
                  "refId": "Task failures"
               }
            ],
            "title": "Task failures",
            "type": "timeseries"
         },
         {
            "datasource": {
               "type": "datasource",
               "uid": "-- Mixed --"
            },
            "description": "The average time taken for recent successful DAG runs by DAG ID in the Apache Airflow system.",
            "fieldConfig": {
               "defaults": {
                  "min": 0,
                  "thresholds": {
                     "steps": [
                        {
                           "color": "green",
                           "value": null
                        },
                        {
                           "color": "red",
                           "value": 80
                        }
                     ]
                  },
                  "unit": "s"
               }
            },
            "gridPos": {
               "h": 8,
               "w": 12,
               "x": 0,
               "y": 9
            },
            "id": 5,
            "options": {
               "legend": {
                  "placement": "right"
               },
               "tooltip": {
                  "mode": "multi"
               }
            },
            "pluginVersion": "v11.4.0",
            "targets": [
               {
                  "datasource": {
                     "type": "prometheus",
                     "uid": "${datasource}"
                  },
                  "expr": "increase(airflow_dagrun_duration_success_sum{job=~\"$job\", instance=~\"$instance\",job=~\"$job\",instance=~\"$instance\", dag_id=~\"$dag_id\"}[$__interval:])/clamp_min(increase(airflow_dagrun_duration_success_count{job=~\"$job\", instance=~\"$instance\",job=~\"$job\",instance=~\"$instance\", dag_id=~\"$dag_id\"}[$__interval:]),1)",
                  "format": "time_series",
                  "instant": false,
                  "interval": "1m",
                  "legendFormat": "{{dag_id}}",
                  "refId": "Average successful DAG run duration"
               }
            ],
            "title": "DAG success duration",
            "type": "timeseries"
         },
         {
            "datasource": {
               "type": "datasource",
               "uid": "-- Mixed --"
            },
            "description": "The average time taken for recent failed DAG runs by DAG ID in the Apache Airflow system.",
            "fieldConfig": {
               "defaults": {
                  "min": 0,
                  "thresholds": {
                     "steps": [
                        {
                           "color": "green",
                           "value": null
                        },
                        {
                           "color": "red",
                           "value": 80
                        }
                     ]
                  },
                  "unit": "s"
               }
            },
            "gridPos": {
               "h": 8,
               "w": 12,
               "x": 12,
               "y": 9
            },
            "id": 6,
            "options": {
               "legend": {
                  "placement": "right"
               },
               "tooltip": {
                  "mode": "multi"
               }
            },
            "pluginVersion": "v11.4.0",
            "targets": [
               {
                  "datasource": {
                     "type": "prometheus",
                     "uid": "${datasource}"
                  },
                  "expr": "increase(airflow_dagrun_duration_failed_sum{job=~\"$job\", instance=~\"$instance\",job=~\"$job\",instance=~\"$instance\", dag_id=~\"$dag_id\"}[$__interval:])/clamp_min(increase(airflow_dagrun_duration_failed_count{job=~\"$job\", instance=~\"$instance\",job=~\"$job\",instance=~\"$instance\", dag_id=~\"$dag_id\"}[$__interval:]),1)",
                  "format": "time_series",
                  "instant": false,
                  "interval": "1m",
                  "legendFormat": "{{dag_id}}",
                  "refId": "Average failed DAG run duration"
               }
            ],
            "title": "DAG failed duration",
            "type": "timeseries"
         },
         {
            "datasource": {
               "type": "datasource",
               "uid": "-- Mixed --"
            },
            "description": "The average time taken for recent task runs by Task ID in the Apache Airflow system.",
            "fieldConfig": {
               "defaults": {
                  "color": {
                     "mode": "thresholds"
                  },
                  "min": 0,
                  "thresholds": {
                     "steps": [
                        {
                           "color": "green",
                           "value": null
                        }
                     ]
                  },
                  "unit": "s"
               }
            },
            "gridPos": {
               "h": 8,
               "w": 8,
               "x": 0,
               "y": 17
            },
            "id": 7,
            "options": {
               "displayMode": "gradient",
               "orientation": "horizontal",
               "reduceOptions": {
                  "calcs": [
                     "lastNotNull"
                  ]
               },
               "showUnfilled": true
            },
            "pluginVersion": "v11.4.0",
            "targets": [
               {
                  "datasource": {
                     "type": "prometheus",
                     "uid": "${datasource}"
                  },
                  "expr": "increase(airflow_dag_task_duration_sum{job=~\"$job\", instance=~\"$instance\",job=~\"$job\",instance=~\"$instance\", dag_id=~\"$dag_id\", task_id=~\"$task_id\"}[$__interval:])/clamp_min(increase(airflow_dag_task_duration_count{job=~\"$job\", instance=~\"$instance\",job=~\"$job\",instance=~\"$instance\", dag_id=~\"$dag_id\", task_id=~\"$task_id\"}[$__interval:]),1)",
                  "format": "time_series",
                  "instant": false,
                  "interval": "1m",
                  "legendFormat": "{{dag_id}} - {{task_id}}",
                  "refId": "Average task duration"
               }
            ],
            "title": "Task duration",
            "type": "bargauge"
         },
         {
            "datasource": {
               "type": "datasource",
               "uid": "-- Mixed --"
            },
            "description": "The number of task counts by DAG ID in the Apache Airflow system.",
            "gridPos": {
               "h": 8,
               "w": 8,
               "x": 8,
               "y": 17
            },
            "id": 8,
            "options": {
               "legend": {
                  "placement": "right"
               },
               "reduceOptions": {
                  "calcs": [
                     "sum"
                  ]
               },
               "tooltip": {
                  "mode": "multi"
               }
            },
            "pluginVersion": "v11.4.0",
            "targets": [
               {
                  "datasource": {
                     "type": "prometheus",
                     "uid": "${datasource}"
                  },
                  "expr": "sum by(job, instance, dag_id, state) (\n  rate(airflow_task_finish_total{job=~\"$job\", instance=~\"$instance\",job=~\"$job\",instance=~\"$instance\", dag_id=~\"$dag_id\", task_id=~\"$task_id\", state=~\"$state\"}[$__rate_interval])\n)",
                  "format": "time_series",
                  "instant": false,
                  "interval": "1m",
                  "legendFormat": "{{dag_id}} - {{state}}",
                  "refId": "Task finish total"
               }
            ],
            "title": "Task count summary",
            "type": "piechart"
         },
         {
            "datasource": {
               "type": "datasource",
               "uid": "-- Mixed --"
            },
            "description": "The number of task counts by Task ID in the Apache Airflow system.",
            "fieldConfig": {
               "defaults": {
                  "decimals": 0,
                  "min": 0,
                  "thresholds": {
                     "steps": [
                        {
                           "color": "green",
                           "value": null
                        }
                     ]
                  },
                  "unit": "none"
               }
            },
            "gridPos": {
               "h": 8,
               "w": 8,
               "x": 16,
               "y": 17
            },
            "id": 9,
            "options": {
               "legend": {
                  "placement": "right"
               },
               "tooltip": {
                  "mode": "multi"
               }
            },
            "pluginVersion": "v11.4.0",
            "targets": [
               {
                  "datasource": {
                     "type": "prometheus",
                     "uid": "${datasource}"
                  },
                  "expr": "rate(airflow_task_finish_total{job=~\"$job\", instance=~\"$instance\",job=~\"$job\",instance=~\"$instance\", dag_id=~\"$dag_id\", task_id=~\"$task_id\", state=~\"$state\"}[$__rate_interval])",
                  "format": "time_series",
                  "instant": false,
                  "interval": "1m",
                  "legendFormat": "{{dag_id}} - {{task_id}} - {{state}}",
                  "refId": "Task finish total"
               }
            ],
            "title": "Task counts",
            "type": "timeseries"
         },
         {
            "collapsed": false,
            "gridPos": {
               "h": 1,
               "w": 0,
               "x": 0,
