      "templating": {
         "list": [
            {
               "label": "Data source",
               "name": "datasource",
               "query": "prometheus",
               "regex": "",
               "type": "datasource"
            },
            {
               "allValue": ".+",
               "datasource": {
                  "type": "prometheus",
                  "uid": "${datasource}"
               },
               "includeAll": true,
               "label": "Job",
               "multi": true,
               "name": "job",
               "query": "label_values(mongodb_network_bytesIn{job=~\"$job\", cl_name=~\"$cl_name\"}, job)",
               "refresh": 2,
               "sort": 1,
               "type": "query"
            },
            {
               "allValue": ".+",
               "datasource": {
                  "type": "prometheus",
                  "uid": "${datasource}"
               },
               "includeAll": true,
               "label": "Cl_name",
               "multi": true,
               "name": "cl_name",
               "query": "label_values(mongodb_network_bytesIn{job=~\"$job\", cl_name=~\"$cl_name\",job=~\"$job\"}, cl_name)",
               "refresh": 2,
               "sort": 1,
               "type": "query"
            },
            {
               "allValue": ".+",
               "datasource": {
                  "type": "prometheus",
                  "uid": "${datasource}"
               },
               "includeAll": true,
               "label": "Instance",
               "multi": true,
               "name": "instance",
               "query": "label_values(mongodb_network_bytesIn{job=~\"$job\", cl_name=~\"$cl_name\",job=~\"$job\",cl_name=~\"$cl_name\"}, instance)",
               "refresh": 2,
               "sort": 1,
               "type": "query"
            }
         ]
