               "defaults": {
                  "thresholds": {
                     "steps": [
                        {
                           "color": "light-green",
                           "value": 0
                        },
                        {
                           "color": "#EAB839",
                           "value": 0.69999999999999996
                        },
                        {
                           "color": "light-red",
                           "value": 0.80000000000000004
                        }
                     ]
                  },
                  "unit": "percentunit"
