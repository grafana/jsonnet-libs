                     "align": "left",
                     "cellOptions": {
                        "type": "auto"
                     },
                     "inspect": false
                  },
                  "mappings": [
                     {
                        "options": {
                           "Health=1": {
                              "index": 0,
                              "text": "Healthy"
                           }
                        },
                        "type": "value"
                     }
                  ],
                  "thresholds": {
                     "mode": "absolute",
                     "steps": [
                        {
                           "color": "green",
                           "value": null
                        }
                     ]
                  },
                  "unit": "reqps"
               },
               "overrides": [
                  {
                     "matcher": {
                        "id": "byName",
                        "options": "Health"
                     },
                     "properties": [
                        {
                           "id": "custom.cellOptions",
                           "value": {
                              "type": "color-text"
                           }
                        },
                        {
                           "id": "mappings",
                           "value": [
                              {
                                 "options": {
                                    "0": {
                                       "color": "red",
                                       "index": 1,
                                       "text": "Unhealthy"
                                    },
                                    "1": {
                                       "color": "green",
                                       "index": 0,
                                       "text": "Healthy"
                                    }
