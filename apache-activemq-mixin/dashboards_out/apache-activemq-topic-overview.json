      "links": [
         {
            "keepTime": true,
            "title": "Apache ActiveMQ Cluster Overview",
            "type": "link",
            "url": "/d/apacheactivemq_cluster_overview"
         },
         {
            "keepTime": true,
            "title": "Apache ActiveMQ Queues",
            "type": "link",
            "url": "/d/apacheactivemq_queues"
         },
         {
            "keepTime": true,
            "title": "Logs",
            "type": "link",
            "url": "/d/apacheactivemq-logs"
         },
         {
            "asDropdown": true,
            "includeVars": true,
            "keepTime": true,
            "tags": [
               "apache-activemq"
            ],
            "title": "All dashboards",
            "type": "dashboards"
         }
      ],
      "panels": [
         {
            "collapsed": false,
            "gridPos": {
               "h": 1,
               "w": 0,
               "x": 0,
               "y": 0
            },
            "id": 1,
            "panels": [ ],
            "title": "Topics",
            "type": "row"
         },
         {
            "datasource": {
               "type": "prometheus",
               "uid": "${prometheus_datasource}"
            },
            "description": "Number of active topics (excluding advisory topics).",
            "fieldConfig": {
               "defaults": {
                  "color": {
                     "fixedColor": "text",
                     "mode": "fixed"
                  }
               }
            },
            "gridPos": {
               "h": 8,
               "w": 6,
               "x": 0,
               "y": 1
            },
            "id": 2,
            "pluginVersion": "v11.0.0",
            "targets": [
               {
                  "datasource": {
                     "type": "prometheus",
                     "uid": "${prometheus_datasource}"
                  },
                  "expr": "count (activemq_topic_queue_size{job=~\"integrations/apache-activemq\",job=~\"$job\",cluster=~\"$cluster\",instance=~\"$instance\", destination!~\"ActiveMQ.Advisory.*\"})",
                  "format": "time_series",
                  "instant": false,
                  "legendFormat": "__auto",
                  "refId": "Topic count"
               }
            ],
            "title": "Topic count",
            "type": "stat"
         },
         {
            "datasource": {
               "type": "prometheus",
               "uid": "${prometheus_datasource}"
            },
            "description": "Total number of topic producers.",
            "fieldConfig": {
               "defaults": {
                  "color": {
                     "fixedColor": "text",
                     "mode": "fixed"
                  }
               }
            },
            "gridPos": {
               "h": 8,
               "w": 6,
               "x": 6,
               "y": 1
            },
            "id": 3,
            "pluginVersion": "v11.0.0",
            "targets": [
               {
                  "datasource": {
                     "type": "prometheus",
                     "uid": "${prometheus_datasource}"
                  },
                  "expr": "sum (activemq_topic_producer_count{job=~\"integrations/apache-activemq\",job=~\"$job\",cluster=~\"$cluster\",instance=~\"$instance\", destination!~\"ActiveMQ.Advisory.*\"})",
                  "format": "time_series",
                  "instant": false,
                  "legendFormat": "__auto",
                  "refId": "Total producers"
               }
            ],
            "title": "Producers",
            "type": "stat"
         },
         {
            "datasource": {
               "type": "prometheus",
               "uid": "${prometheus_datasource}"
            },
            "description": "Total number of topic consumers.",
            "fieldConfig": {
               "defaults": {
                  "color": {
                     "fixedColor": "text",
                     "mode": "fixed"
                  }
               }
            },
            "gridPos": {
               "h": 8,
               "w": 6,
               "x": 12,
               "y": 1
            },
            "id": 4,
            "pluginVersion": "v11.0.0",
            "targets": [
               {
                  "datasource": {
                     "type": "prometheus",
                     "uid": "${prometheus_datasource}"
                  },
                  "expr": "sum (activemq_topic_consumer_count{job=~\"integrations/apache-activemq\",job=~\"$job\",cluster=~\"$cluster\",instance=~\"$instance\", destination!~\"ActiveMQ.Advisory.*\"})",
                  "format": "time_series",
                  "instant": false,
                  "legendFormat": "__auto",
                  "refId": "Total consumers"
               }
            ],
            "title": "Consumers",
            "type": "stat"
         },
         {
            "datasource": {
               "type": "prometheus",
               "uid": "${prometheus_datasource}"
            },
            "description": "Average number of consumers per topic.",
            "fieldConfig": {
               "defaults": {
                  "color": {
                     "fixedColor": "text",
                     "mode": "fixed"
                  }
               }
            },
            "gridPos": {
               "h": 8,
               "w": 6,
               "x": 18,
               "y": 1
            },
            "id": 5,
            "pluginVersion": "v11.0.0",
            "targets": [
               {
                  "datasource": {
                     "type": "prometheus",
                     "uid": "${prometheus_datasource}"
                  },
                  "expr": "avg (activemq_topic_consumer_count{job=~\"integrations/apache-activemq\",job=~\"$job\",cluster=~\"$cluster\",instance=~\"$instance\", destination!~\"ActiveMQ.Advisory.*\"})",
                  "format": "time_series",
                  "instant": false,
                  "legendFormat": "__auto",
                  "refId": "Average consumers per topic"
               }
            ],
            "title": "Average consumers per topic",
            "type": "stat"
         },
         {
            "datasource": {
               "type": "prometheus",
               "uid": "${prometheus_datasource}"
            },
            "description": "Rate of messages being enqueued to topics.",
            "fieldConfig": {
               "defaults": {
                  "custom": {
                     "fillOpacity": 0,
                     "gradientMode": "opacity",
                     "lineInterpolation": "smooth",
                     "lineWidth": 2,
                     "showPoints": "never",
                     "spanNulls": false
                  },
                  "unit": "/ sec"
               }
            },
            "gridPos": {
               "h": 8,
               "w": 12,
               "x": 0,
               "y": 9
            },
            "id": 6,
            "options": {
               "legend": {
                  "calcs": [ ],
                  "displayMode": "list"
               },
               "tooltip": {
                  "mode": "multi",
                  "sort": "desc"
               }
            },
            "pluginVersion": "v11.0.0",
            "targets": [
               {
                  "datasource": {
                     "type": "prometheus",
                     "uid": "${prometheus_datasource}"
                  },
                  "expr": "topk by (instance, activemq_cluster, job) ($k_selector, rate(activemq_topic_enqueue_count{job=~\"integrations/apache-activemq\",job=~\"$job\",cluster=~\"$cluster\",instance=~\"$instance\", destination!~\"ActiveMQ.Advisory.*\"}[$__rate_interval]))",
                  "format": "time_series",
                  "instant": false,
                  "interval": "1m",
                  "legendFormat": "{{activemq_cluster}} - {{instance}} - {{destination}}",
                  "refId": "Top topics by enqueue rate"
               }
            ],
            "title": "Top topics by enqueue rate",
            "type": "timeseries"
         },
         {
            "datasource": {
               "type": "prometheus",
               "uid": "${prometheus_datasource}"
            },
            "description": "Rate of messages being dequeued from topics.",
            "fieldConfig": {
               "defaults": {
                  "custom": {
                     "fillOpacity": 0,
                     "gradientMode": "opacity",
                     "lineInterpolation": "smooth",
                     "lineWidth": 2,
                     "showPoints": "never",
                     "spanNulls": false
                  },
                  "unit": "/ sec"
               }
            },
            "gridPos": {
               "h": 8,
               "w": 12,
               "x": 12,
               "y": 9
            },
            "id": 7,
            "options": {
               "legend": {
                  "calcs": [ ],
                  "displayMode": "list"
               },
               "tooltip": {
                  "mode": "multi",
                  "sort": "desc"
               }
            },
            "pluginVersion": "v11.0.0",
            "targets": [
               {
                  "datasource": {
                     "type": "prometheus",
                     "uid": "${prometheus_datasource}"
                  },
                  "expr": "topk by (instance, activemq_cluster, job) ($k_selector, rate(activemq_topic_dequeue_count{job=~\"integrations/apache-activemq\",job=~\"$job\",cluster=~\"$cluster\",instance=~\"$instance\", destination!~\"ActiveMQ.Advisory.*\"}[$__rate_interval]))",
                  "format": "time_series",
                  "instant": false,
                  "interval": "1m",
                  "legendFormat": "{{activemq_cluster}} - {{instance}} - {{destination}}",
                  "refId": "Top topics by dequeue rate"
               }
            ],
            "title": "Top topics by dequeue rate",
            "type": "timeseries"
         },
         {
            "datasource": {
               "type": "prometheus",
               "uid": "${prometheus_datasource}"
            },
            "description": "Average time to enqueue messages to topics.",
            "fieldConfig": {
               "defaults": {
                  "custom": {
                     "fillOpacity": 0,
                     "gradientMode": "opacity",
                     "lineInterpolation": "smooth",
                     "lineWidth": 2,
                     "showPoints": "never",
                     "spanNulls": false
                  },
                  "unit": "ms"
               }
            },
            "gridPos": {
               "h": 8,
               "w": 8,
               "x": 0,
               "y": 17
            },
            "id": 8,
            "options": {
               "legend": {
                  "calcs": [ ],
                  "displayMode": "list"
               },
               "tooltip": {
                  "mode": "multi",
                  "sort": "desc"
               }
            },
            "pluginVersion": "v11.0.0",
            "targets": [
               {
                  "datasource": {
                     "type": "prometheus",
                     "uid": "${prometheus_datasource}"
                  },
                  "expr": "topk by (instance, activemq_cluster, job) ($k_selector, activemq_topic_average_enqueue_time{job=~\"integrations/apache-activemq\",job=~\"$job\",cluster=~\"$cluster\",instance=~\"$instance\", destination!~\"ActiveMQ.Advisory.*\"})",
                  "format": "time_series",
                  "instant": false,
                  "legendFormat": "{{activemq_cluster}} - {{instance}} - {{destination}}",
                  "refId": "Top topics by average enqueue time"
               }
            ],
            "title": "Top topics by average enqueue time",
            "type": "timeseries"
         },
         {
            "datasource": {
               "type": "prometheus",
               "uid": "${prometheus_datasource}"
            },
            "description": "Rate of messages expiring in topics.",
            "fieldConfig": {
               "defaults": {
                  "custom": {
                     "fillOpacity": 0,
                     "gradientMode": "opacity",
                     "lineInterpolation": "smooth",
                     "lineWidth": 2,
                     "showPoints": "never",
                     "spanNulls": false
                  },
                  "unit": "/ sec"
               }
            },
            "gridPos": {
               "h": 8,
               "w": 8,
               "x": 8,
               "y": 17
            },
            "id": 9,
            "options": {
               "legend": {
                  "calcs": [ ],
                  "displayMode": "list"
               },
               "tooltip": {
                  "mode": "multi",
                  "sort": "desc"
               }
            },
            "pluginVersion": "v11.0.0",
            "targets": [
               {
                  "datasource": {
                     "type": "prometheus",
                     "uid": "${prometheus_datasource}"
                  },
                  "expr": "topk by (instance, activemq_cluster, job) ($k_selector, activemq_topic_expired_count{job=~\"integrations/apache-activemq\",job=~\"$job\",cluster=~\"$cluster\",instance=~\"$instance\", destination!~\"ActiveMQ.Advisory.*\"})",
                  "format": "time_series",
                  "instant": false,
                  "interval": "1m",
                  "legendFormat": "{{activemq_cluster}} - {{instance}} - {{destination}}",
                  "refId": "Top topics by expired rate"
               }
            ],
            "title": "Top topics by expired rate",
            "type": "timeseries"
         },
         {
            "datasource": {
               "type": "prometheus",
               "uid": "${prometheus_datasource}"
            },
            "description": "Average size of messages in topics.",
            "fieldConfig": {
               "defaults": {
                  "custom": {
                     "fillOpacity": 54,
                     "gradientMode": "opacity",
                     "lineInterpolation": "smooth",
                     "lineWidth": 2,
                     "showPoints": "never",
                     "spanNulls": false
                  },
                  "unit": "bytes"
               }
            },
            "gridPos": {
               "h": 8,
               "w": 8,
               "x": 16,
               "y": 17
            },
            "id": 10,
            "options": {
               "legend": {
                  "calcs": [ ],
                  "displayMode": "list"
               },
               "tooltip": {
                  "mode": "multi",
                  "sort": "desc"
               }
            },
            "pluginVersion": "v11.0.0",
            "targets": [
               {
                  "datasource": {
                     "type": "prometheus",
                     "uid": "${prometheus_datasource}"
                  },
                  "expr": "activemq_topic_average_message_size{job=~\"integrations/apache-activemq\",job=~\"$job\",cluster=~\"$cluster\",instance=~\"$instance\", destination!~\"ActiveMQ.Advisory.*\"}",
                  "format": "time_series",
                  "instant": false,
                  "legendFormat": "{{activemq_cluster}} - {{instance}} - {{destination}}",
                  "refId": "Average message size"
               }
            ],
            "title": "Topic average message size",
            "type": "timeseries"
         },
         {
            "datasource": {
               "type": "prometheus",
               "uid": "${prometheus_datasource}"
            },
            "description": "Summary of topics showing topic name, enqueue and dequeue rate, average enqueue time, and average message size.",
            "fieldConfig": {
               "defaults": {
                  "custom": {
                     "align": "left",
                     "cellOptions": {
                        "type": "auto"
                     },
                     "inspect": false
                  },
                  "thresholds": {
                     "mode": "absolute",
                     "steps": [
                        {
                           "color": "green"
                        }
                     ]
                  }
               },
               "overrides": [
                  {
                     "matcher": {
                        "id": "byName",
                        "options": "Enqueue rate"
                     },
                     "properties": [
                        {
                           "id": "unit",
                           "value": "mps"
                        }
                     ]
                  },
                  {
                     "matcher": {
                        "id": "byName",
                        "options": "Dequeue rate"
                     },
                     "properties": [
                        {
                           "id": "unit",
                           "value": "mps"
                        }
                     ]
                  },
                  {
                     "matcher": {
                        "id": "byName",
                        "options": "Average enqueue time"
                     },
                     "properties": [
                        {
                           "id": "unit",
                           "value": "ms"
                        }
                     ]
                  },
                  {
                     "matcher": {
                        "id": "byName",
                        "options": "Average message size"
                     },
                     "properties": [
                        {
                           "id": "unit",
                           "value": "decbytes"
                        }
                     ]
                  },
                  {
                     "matcher": {
                        "id": "byName",
                        "options": "ActiveMQ cluster"
                     },
                     "properties": [
                        {
                           "id": "links",
                           "value": [
                              {
                                 "title": "Cluster link",
                                 "url": "d/apacheactivemq_cluster_overview?var-activemq_cluster=${__data.fields.activemq_cluster}&${__url_time_range}&var-datasource=${datasource}"
                              }
                           ]
                        }
                     ]
                  },
                  {
                     "matcher": {
                        "id": "byName",
                        "options": "Instance"
                     },
                     "properties": [
                        {
                           "id": "links",
                           "value": [
                              {
                                 "title": "Instance link",
                                 "url": "d/apacheactivemq_instance_overview?var-instance=${__data.fields.instance}&${__url_time_range}&var-datasource=${datasource}"
                              }
                           ]
                        }
                     ]
                  }
               ]
            },
            "gridPos": {
               "h": 8,
               "w": 24,
               "x": 0,
               "y": 25
            },
            "id": 11,
            "options": {
               "cellHeight": "sm",
               "footer": {
                  "countRows": false,
                  "fields": [
                     ""
                  ],
                  "reducer": [
                     "sum"
                  ],
                  "show": false
               },
               "showHeader": true
            },
            "pluginVersion": "v11.0.0",
            "targets": [
               {
                  "datasource": {
                     "type": "prometheus",
                     "uid": "${prometheus_datasource}"
                  },
                  "expr": "rate(activemq_topic_enqueue_count{job=~\"integrations/apache-activemq\",job=~\"$job\",cluster=~\"$cluster\",instance=~\"$instance\", destination!~\"ActiveMQ.Advisory.*\", destination=~\".*$name.*\"}[$__rate_interval])",
                  "format": "table",
                  "instant": true,
                  "interval": "1m",
                  "legendFormat": "{{instance}}: Enqueue rate",
                  "refId": "Enqueue rate"
               },
               {
                  "datasource": {
                     "type": "prometheus",
                     "uid": "${prometheus_datasource}"
                  },
                  "expr": "rate(activemq_topic_dequeue_count{job=~\"integrations/apache-activemq\",job=~\"$job\",cluster=~\"$cluster\",instance=~\"$instance\", destination!~\"ActiveMQ.Advisory.*\", destination=~\".*$name.*\"}[$__rate_interval])",
                  "format": "table",
                  "instant": true,
                  "interval": "1m",
                  "legendFormat": "{{instance}}: Dequeue rate",
                  "refId": "Dequeue rate"
               },
               {
                  "datasource": {
                     "type": "prometheus",
                     "uid": "${prometheus_datasource}"
                  },
                  "expr": "activemq_topic_average_enqueue_time{job=~\"integrations/apache-activemq\",job=~\"$job\",cluster=~\"$cluster\",instance=~\"$instance\", destination!~\"ActiveMQ.Advisory.*\", destination=~\".*$name.*\"}",
                  "format": "table",
                  "instant": true,
                  "legendFormat": "{{instance}}: Average enqueue time",
                  "refId": "Average enqueue time"
               },
               {
                  "datasource": {
                     "type": "prometheus",
                     "uid": "${prometheus_datasource}"
                  },
                  "expr": "activemq_topic_average_message_size{job=~\"integrations/apache-activemq\",job=~\"$job\",cluster=~\"$cluster\",instance=~\"$instance\", destination!~\"ActiveMQ.Advisory.*\", destination=~\".*$name.*\"}",
                  "format": "table",
                  "instant": true,
                  "legendFormat": "{{instance}}: Average message size",
                  "refId": "Average message size"
               }
            ],
            "title": "Topic summary",
            "transformations": [
               {
                  "id": "joinByField",
                  "options": {
                     "byField": "destination",
                     "mode": "outer"
                  }
               },
               {
                  "id": "organize",
                  "options": {
                     "indexByName": { },
                     "renameByName": {
                        "Value #Average enqueue time": "Average enqueue time",
                        "Value #Average message size": "Average message size",
                        "Value #Dequeue rate": "Dequeue rate",
                        "Value #Enqueue rate": "Enqueue rate",
                        "activemq_cluster 1": "ActiveMQ cluster",
                        "destination": "Destination",
                        "instance 1": "Instance"
                     }
                  }
               },
               {
                  "id": "filterFieldsByName",
                  "options": {
                     "include": {
                        "names": [
                           "ActiveMQ cluster",
                           "Instance",
                           "Enqueue rate",
                           "Dequeue rate",
                           "Average enqueue time",
                           "Average message size",
                           "Destination"
                        ]
                     }
                  }
               }
            ],
            "type": "table"
         }
      ],
      "refresh": "1m",
      "schemaVersion": 39,
      "tags": [
         "apache-activemq"
      ],
      "templating": {
         "list": [
            {
               "label": "Prometheus data source",
               "name": "prometheus_datasource",
               "query": "prometheus",
               "regex": "",
               "type": "datasource"
            },
            {
               "allValue": ".+",
