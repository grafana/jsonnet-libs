         {
            "datasource": {
               "type": "prometheus",
               "uid": "${prometheus_datasource}"
            },
            "description": "Number of active topics (excluding advisory topics).",
            "fieldConfig": {
               "defaults": {
                  "color": {
                     "fixedColor": "text",
                     "mode": "fixed"
                  }
               }
            },
            "gridPos": {
               "h": 8,
               "w": 6,
               "x": 0,
               "y": 1
            },
            "id": 2,
            "pluginVersion": "v11.0.0",
            "targets": [
               {
                  "datasource": {
                     "type": "prometheus",
                     "uid": "${prometheus_datasource}"
                  },
                  "expr": "count (activemq_topic_queue_size{job=~\"integrations/apache-activemq\",job=~\"$job\",cluster=~\"$cluster\",instance=~\"$instance\", destination!~\"ActiveMQ.Advisory.*\"})",
                  "format": "time_series",
                  "instant": false,
                  "legendFormat": "__auto",
                  "refId": "Topic count"
               }
            ],
            "title": "Topic count",
            "type": "stat"
         },
         {
            "datasource": {
               "type": "prometheus",
               "uid": "${prometheus_datasource}"
            },
            "description": "Total number of topic producers.",
            "fieldConfig": {
               "defaults": {
                  "color": {
                     "fixedColor": "text",
                     "mode": "fixed"
