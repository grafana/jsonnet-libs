            "fieldConfig": {
               "defaults": {
                  "color": {
                     "fixedColor": "#5794f2",
                     "mode": "palette-classic"
                  },
                  "custom": {
                     "fillOpacity": 30,
                     "gradientMode": "opacity",
                     "lineInterpolation": "smooth",
                     "lineWidth": 2,
                     "showPoints": "never"
                  },
                  "decimals": 1,
