                  },
                  "custom": {
                     "fillOpacity": 30,
                     "gradientMode": "opacity",
                     "lineInterpolation": "smooth",
                     "lineWidth": 2,
                     "showPoints": "never"
                  },
                  "decimals": 1,
                  "noValue": "No packets",
                  "thresholds": {
                     "mode": "absolute",
                     "steps": [ ]
                  },
                  "unit": "s"
               }
            },
            "gridPos": {
               "h": 6,
               "w": 12,
