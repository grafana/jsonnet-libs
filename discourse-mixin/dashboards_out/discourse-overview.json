               "h": 6,
               "w": 12,
               "x": 0,
               "y": 20
            },
            "id": 9,
            "pluginVersion": "v11.4.0",
            "targets": [
               {
                  "datasource": {
                     "type": "prometheus",
                     "uid": "${datasource}"
                  },
                  "expr": "sum by (instance, job, quantile) (discourse_http_duration_seconds{job=\"integrations/discourse\",job=~\"$job\",instance=~\"$instance\",action=\"latest\"})",
                  "format": "time_series",
                  "instant": false,
                  "legendFormat": "{{ instance }}",
                  "refId": "Request time percentiles"
               }
