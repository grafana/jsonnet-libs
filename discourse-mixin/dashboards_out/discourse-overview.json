                  },
                  "expr": "sum by (job) (\n  discourse_http_duration_seconds{job=\"integrations/discourse\",job=~\"$job\",instance=~\"$instance\",quantile=\"0.5\",action=\"latest\"}\n)",
                  "format": "time_series",
                  "instant": false,
                  "legendFormat": "{{ instance }}",
                  "refId": "Latest median request time"
               }
            ],
            "title": "Latest median request time",
            "type": "timeseries"
         },
         {
            "datasource": {
               "type": "prometheus",
               "uid": "${datasource}"
            },
