                  "expr": "jvm_memory_usage_used_bytes{job=\"integrations/tomcat\",job=~\"$job\",cluster=~\"$cluster\",instance=~\"$instance\"}",
                  "format": "time_series",
                  "instant": false,
                  "legendFormat": "{{ instance }} - {{area}}",
                  "refId": "Memory usage"
