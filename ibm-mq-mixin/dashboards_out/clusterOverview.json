                     "reduceOptions": {
                        "calcs": [
                           "lastNotNull"
                        ]
                     },
                     "textMode": "auto"
                  },
                  "pluginVersion": "v11.0.0",
                  "targets": [
                     {
                        "datasource": {
                           "type": "prometheus",
                           "uid": "${prometheus_datasource}"
