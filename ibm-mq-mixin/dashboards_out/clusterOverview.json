                  },
                  "gridPos": {
                     "h": 7,
                     "w": 4,
                     "x": 8,
                     "y": 1
                  },
                  "id": 4,
                  "options": {
                     "colorMode": "value",
                     "graphMode": "none",
                     "reduceOptions": {
                        "calcs": [
                           "lastNotNull"
                        ]
                     },
                     "textMode": "auto"
                  },
                  "pluginVersion": "v11.0.0",
                  "targets": [
                     {
                        "datasource": {
                           "type": "prometheus",
                           "uid": "${prometheus_datasource}"
