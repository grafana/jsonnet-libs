               {
                  "datasource": {
                     "type": "datasource",
                     "uid": "-- Mixed --"
                  },
                  "description": "The number of queue operations of the cluster. ",
                  "fieldConfig": {
                     "defaults": {
                        "unit": "operations"
                     },
                     "overrides": [
                        {
                           "matcher": {
                              "id": "byName",
                              "options": "MQINQ"
                           },
                           "properties": [
                              {
                                 "id": "custom.hideFrom",
                                 "value": {
                                    "legend": false,
                                    "tooltip": false,
                                    "viz": false
                                 }
                              }
                           ]
                        },
                        {
                           "matcher": {
                              "id": "byName",
                              "options": "MQGET"
                           },
                           "properties": [
                              {
                                 "id": "custom.hideFrom",
                                 "value": {
                                    "legend": false,
                                    "tooltip": false,
                                    "viz": false
                                 }
                              }
                           ]
                        },
                        {
                           "matcher": {
                              "id": "byName",
                              "options": "MQOPEN"
                           },
                           "properties": [
                              {
                                 "id": "custom.hideFrom",
                                 "value": {
                                    "legend": false,
                                    "tooltip": false,
                                    "viz": false
                                 }
                              }
                           ]
                        },
                        {
                           "matcher": {
                              "id": "byName",
                              "options": "MQPUT/MQPUT1"
                           },
                           "properties": [
                              {
                                 "id": "custom.hideFrom",
                                 "value": {
                                    "legend": false,
                                    "tooltip": false,
                                    "viz": false
                                 }
                              }
                           ]
                        }
