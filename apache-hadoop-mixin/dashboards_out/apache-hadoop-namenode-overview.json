            "description": "The storage utilization of the NameNode.",
            "fieldConfig": {
               "defaults": {
                  "custom": {
                     "fillOpacity": 0,
                     "gradientMode": "opacity",
                     "lineInterpolation": "smooth",
                     "lineWidth": 2,
                     "showPoints": "never",
                     "spanNulls": false
                  },
                  "unit": "percent"
               }
            },
            "gridPos": {
               "h": 8,
               "w": 12,
               "x": 12,
               "y": 1
