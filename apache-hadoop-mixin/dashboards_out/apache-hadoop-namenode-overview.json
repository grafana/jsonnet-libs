            "id": 3,
            "options": {
               "legend": {
                  "calcs": [ ],
                  "displayMode": "list"
               },
               "tooltip": {
                  "mode": "multi",
                  "sort": "desc"
               }
            },
            "pluginVersion": "v11.0.0",
            "targets": [
               {
                  "datasource": {
                     "type": "prometheus",
                     "uid": "${prometheus_datasource}"
                  },
                  "expr": "100 * hadoop_namenode_capacityused{job=\"integrations/apache-hadoop\",job=~\"$job\",hadoop_cluster=~\"$hadoop_cluster\",cluster=~\"$cluster\",instance=~\"$instance\", name=\"FSNamesystem\"} / clamp_min(hadoop_namenode_capacitytotal{job=\"integrations/apache-hadoop\",job=~\"$job\",hadoop_cluster=~\"$hadoop_cluster\",cluster=~\"$cluster\",instance=~\"$instance\", name=\"FSNamesystem\"}, 1)",
                  "format": "time_series",
                  "instant": false,
                  "legendFormat": "{{hadoop_cluster}} - {{instance}}",
                  "refId": "Capacity utilization"
               }
            ],
            "title": "Capacity utilization",
            "type": "timeseries"
