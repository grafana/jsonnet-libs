            "pluginVersion": "v11.4.0",
            "targets": [
               {
                  "datasource": {
                     "type": "prometheus",
                     "uid": "${prometheus_datasource}"
                  },
                  "expr": "hadoop_namenode_numlivedatanodes{job=\"integrations/apache-hadoop\",job=~\"$job\",hadoop_cluster=~\"$hadoop_cluster\",cluster=~\"$cluster\",instance=~\"$instance\", name=\"FSNamesystem\"}",
                  "format": "time_series",
                  "instant": false,
                  "legendFormat": "{{hadoop_cluster}} - {{instance}} - live DataNodes",
                  "refId": "DataNode state live"
               },
               {
                  "datasource": {
                     "type": "prometheus",
                     "uid": "${prometheus_datasource}"
                  },
                  "expr": "hadoop_namenode_numdeaddatanodes{job=\"integrations/apache-hadoop\",job=~\"$job\",hadoop_cluster=~\"$hadoop_cluster\",cluster=~\"$cluster\",instance=~\"$instance\", name=\"FSNamesystem\"}",
                  "format": "time_series",
                  "instant": false,
                  "legendFormat": "{{hadoop_cluster}} - {{instance}} - dead DataNodes",
                  "refId": "DataNode state dead"
               },
               {
                  "datasource": {
                     "type": "prometheus",
                     "uid": "${prometheus_datasource}"
                  },
                  "expr": "hadoop_namenode_numstaledatanodes{job=\"integrations/apache-hadoop\",job=~\"$job\",hadoop_cluster=~\"$hadoop_cluster\",cluster=~\"$cluster\",instance=~\"$instance\"}",
                  "format": "time_series",
                  "instant": false,
                  "legendFormat": "{{hadoop_cluster}} - {{instance}} - stale DataNodes",
                  "refId": "DataNode state stale"
               },
               {
                  "datasource": {
                     "type": "prometheus",
                     "uid": "${prometheus_datasource}"
                  },
                  "expr": "hadoop_namenode_numdecommissioningdatanodes{job=\"integrations/apache-hadoop\",job=~\"$job\",hadoop_cluster=~\"$hadoop_cluster\",cluster=~\"$cluster\",instance=~\"$instance\", name=\"FSNamesystem\"}",
                  "format": "time_series",
                  "instant": false,
                  "legendFormat": "{{hadoop_cluster}} - {{instance}} - decommissioning DataNodes",
                  "refId": "DataNode state decommissioning"
               }
            ],
            "title": "DataNode state",
            "type": "piechart"
         },
         {
            "datasource": {
               "type": "prometheus",
               "uid": "${prometheus_datasource}"
