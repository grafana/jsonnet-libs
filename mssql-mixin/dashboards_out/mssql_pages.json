{
      "annotations": {
         "list": [ ]
      },
      "editable": false,
      "id": null,
      "links": [
         {
            "keepTime": true,
            "title": "Logs",
            "type": "link",
            "url": "/d/mssql-logs"
         },
         {
            "keepTime": true,
            "title": "MSSQL Overview",
            "type": "link",
            "url": "/d/mssql_mssql_overview"
         },
         {
            "asDropdown": true,
            "includeVars": true,
            "keepTime": true,
            "tags": [
               "mssql"
            ],
            "title": "All dashboards",
            "type": "dashboards"
         }
      ],
      "panels": [
         {
            "collapsed": false,
            "gridPos": {
               "h": 1,
               "w": 0,
               "x": 0,
               "y": 0
            },
            "id": 1,
            "panels": [ ],
            "title": "Pages",
            "type": "row"
         },
         {
            "datasource": {
               "type": "prometheus",
               "uid": "${prometheus_datasource}"
            },
            "description": "Memory used for the OS page file.",
            "fieldConfig": {
               "defaults": {
                  "custom": {
                     "fillOpacity": 50,
                     "gradientMode": "opacity",
                     "lineInterpolation": "smooth",
                     "lineWidth": 2,
                     "showPoints": "never",
                     "spanNulls": false
                  },
                  "unit": "bytes"
               }
            },
            "gridPos": {
               "h": 8,
               "w": 12,
               "x": 0,
               "y": 1
            },
            "id": 2,
            "options": {
               "legend": {
                  "calcs": [ ],
                  "displayMode": "list"
               },
               "tooltip": {
                  "mode": "multi",
                  "sort": "desc"
               }
            },
            "pluginVersion": "v11.0.0",
            "targets": [
               {
                  "datasource": {
                     "type": "prometheus",
                     "uid": "${prometheus_datasource}"
                  },
                  "expr": "mssql_os_page_file{job=~\"integrations/mssql\",job=~\"$job\",cluster=~\"$cluster\",instance=~\"$instance\"}",
                  "format": "time_series",
                  "instant": false,
                  "legendFormat": "{{ instance }} - {{ state }}",
                  "refId": "Page file memory"
               }
            ],
            "title": "Page file memory",
            "type": "timeseries"
         },
         {
            "datasource": {
               "type": "prometheus",
               "uid": "${prometheus_datasource}"
            },
            "description": "Percentage of page found and read from the SQL Server buffer cache.",
            "fieldConfig": {
               "defaults": {
                  "custom": {
                     "fillOpacity": 0,
                     "gradientMode": "opacity",
                     "lineInterpolation": "smooth",
                     "lineWidth": 2,
                     "showPoints": "never",
                     "spanNulls": false
                  },
                  "unit": "percent"
               }
            },
            "gridPos": {
               "h": 8,
               "w": 12,
               "x": 12,
               "y": 1
            },
            "id": 3,
            "options": {
               "legend": {
                  "calcs": [ ],
                  "displayMode": "list"
               },
               "tooltip": {
                  "mode": "multi",
                  "sort": "desc"
               }
            },
            "pluginVersion": "v11.0.0",
            "targets": [
               {
                  "datasource": {
                     "type": "prometheus",
                     "uid": "${prometheus_datasource}"
                  },
                  "expr": "mssql_buffer_cache_hit_ratio{job=~\"integrations/mssql\",job=~\"$job\",cluster=~\"$cluster\",instance=~\"$instance\"}",
                  "format": "time_series",
                  "instant": false,
                  "legendFormat": "{{ instance }}",
                  "refId": "Buffer cache hit percentage"
               }
            ],
            "title": "Buffer cache hit percentage",
            "type": "timeseries"
         },
         {
            "datasource": {
               "type": "prometheus",
               "uid": "${prometheus_datasource}"
            },
            "description": "Rate of page checkpoints per second.",
            "fieldConfig": {
               "defaults": {
                  "custom": {
                     "fillOpacity": 0,
                     "gradientMode": "opacity",
                     "lineInterpolation": "smooth",
                     "lineWidth": 2,
                     "showPoints": "never",
                     "spanNulls": false
                  },
                  "unit": "checkpoints/s"
               }
            },
            "gridPos": {
               "h": 8,
               "w": 12,
               "x": 0,
               "y": 9
            },
            "id": 4,
            "options": {
               "legend": {
                  "calcs": [ ],
                  "displayMode": "list"
               },
               "tooltip": {
                  "mode": "multi",
                  "sort": "desc"
               }
            },
            "pluginVersion": "v11.0.0",
            "targets": [
               {
                  "datasource": {
                     "type": "prometheus",
                     "uid": "${prometheus_datasource}"
                  },
                  "expr": "mssql_checkpoint_pages_sec{job=~\"integrations/mssql\",job=~\"$job\",cluster=~\"$cluster\",instance=~\"$instance\"}",
                  "format": "time_series",
                  "instant": false,
                  "legendFormat": "{{ instance }}",
                  "refId": "Page checkpoints"
               }
            ],
            "title": "Page checkpoints",
            "type": "timeseries"
         },
         {
            "datasource": {
               "type": "prometheus",
               "uid": "${prometheus_datasource}"
            },
            "description": "The number of page faults that were incurred by the SQL Server process.",
            "fieldConfig": {
               "defaults": {
                  "custom": {
                     "fillOpacity": 0,
                     "gradientMode": "opacity",
                     "lineInterpolation": "smooth",
                     "lineWidth": 2,
                     "showPoints": "never",
                     "spanNulls": false
                  },
                  "unit": "faults"
               }
            },
            "gridPos": {
               "h": 8,
               "w": 12,
               "x": 12,
               "y": 9
            },
            "id": 5,
            "options": {
               "legend": {
                  "calcs": [ ],
                  "displayMode": "list"
               },
               "tooltip": {
                  "mode": "multi",
                  "sort": "desc"
               }
            },
            "pluginVersion": "v11.0.0",
            "targets": [
               {
                  "datasource": {
                     "type": "prometheus",
                     "uid": "${prometheus_datasource}"
                  },
                  "expr": "increase(mssql_page_fault_count_total{job=~\"integrations/mssql\",job=~\"$job\",cluster=~\"$cluster\",instance=~\"$instance\"}[$__rate_interval:])",
                  "format": "time_series",
                  "instant": false,
                  "interval": "1m",
                  "legendFormat": "{{ instance }}",
                  "refId": "Page faults"
               }
            ],
            "title": "Page faults",
            "type": "timeseries"
         }
      ],
      "refresh": "30s",
      "schemaVersion": 39,
      "tags": [
         "mssql"
      ],
      "templating": {
         "list": [
            {
               "label": "Prometheus data source",
               "name": "prometheus_datasource",
               "query": "prometheus",
               "regex": "(?!grafanacloud-usage|grafanacloud-ml-metrics).+",
               "type": "datasource"
            },
            {
               "allValue": ".+",
               "datasource": {
                  "type": "prometheus",
                  "uid": "${prometheus_datasource}"
               },
               "includeAll": true,
               "label": "Job",
               "multi": true,
               "name": "job",
               "query": "label_values(mssql_io_stall_seconds_total{job=~\"integrations/mssql\"}, job)",
               "refresh": 2,
               "sort": 1,
               "type": "query"
            },
            {
               "allValue": ".*",
               "datasource": {
                  "type": "prometheus",
                  "uid": "${prometheus_datasource}"
               },
               "includeAll": true,
               "label": "Cluster",
               "multi": true,
               "name": "cluster",
               "query": "label_values(mssql_io_stall_seconds_total{job=~\"integrations/mssql\",job=~\"$job\"}, cluster)",
               "refresh": 2,
               "sort": 1,
               "type": "query"
            },
            {
               "allValue": ".+",
               "datasource": {
                  "type": "prometheus",
                  "uid": "${prometheus_datasource}"
               },
               "includeAll": true,
               "label": "Instance",
               "multi": true,
               "name": "instance",
               "query": "label_values(mssql_io_stall_seconds_total{job=~\"integrations/mssql\",job=~\"$job\",cluster=~\"$cluster\"}, instance)",
               "refresh": 2,
               "sort": 1,
               "type": "query"
            },
            {
               "hide": 2,
               "label": "Loki data source",
               "name": "loki_datasource",
               "query": "loki",
               "regex": "(?!grafanacloud.+usage-insights|grafanacloud.+alert-state-history).+",
               "type": "datasource"
            },
            {
               "allValue": ".*",
               "datasource": {
                  "type": "prometheus",
                  "uid": "${prometheus_datasource}"
               },
               "includeAll": true,
               "label": "DB",
               "multi": true,
               "name": "db",
               "query": "label_values(mssql_io_stall_seconds_total{job=~\"integrations/mssql\"}, db)",
               "refresh": 2,
               "sort": 1,
               "type": "query"
            }
         ]
      },
      "time": {
         "from": "now-30m",
         "to": "now"
      },
      "timezone": "default",
      "title": "MSSQL pages",
      "uid": "mssql_mssql_pages"
   }