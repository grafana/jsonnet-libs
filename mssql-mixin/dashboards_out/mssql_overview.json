                  "expr": "mssql_server_total_memory_bytes{job=~\"integrations/mssql\",job=~\"$job\",cluster=~\"$cluster\",instance=~\"$instance\"}",
                  "format": "time_series",
                  "instant": false,
                  "legendFormat": "{{ instance }} - total",
                  "refId": "Memory manager total"
