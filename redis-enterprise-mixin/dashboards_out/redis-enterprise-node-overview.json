                  "legendFormat": "{{ node }}",
                  "refId": "Node memory utilization"
               },
               {
                  "datasource": {
                     "type": "prometheus",
                     "uid": "${prometheus_datasource}"
                  },
                  "expr": "node_up{job=~\"$job\",redis_cluster=~\"$redis_cluster\",instance=~\"$instance\", node=~\"$node\"}",
                  "format": "time_series",
                  "instant": false,
                  "legendFormat": "{{ node }}",
                  "refId": "Node up"
               }
            ],
            "title": "Nodes",
            "transformations": [
               {
                  "id": "timeSeriesTable"
               },
               {
                  "id": "merge"
               },
               {
                  "id": "renameByRegex",
                  "options": {
                     "regex": "Trend #(.*)",
                     "renamePattern": "$1"
                  }
               },
               {
                  "id": "organize",
                  "options": {
                     "excludeByName": {
                        "__name__": true,
                        "addr": true,
                        "cnm_version": true,
                        "instance": true
