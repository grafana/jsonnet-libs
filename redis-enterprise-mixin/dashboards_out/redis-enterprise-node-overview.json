            "fieldConfig": {
               "defaults": {
                  "custom": {
                     "filterable": false
                  }
               },
               "overrides": [
                  {
                     "matcher": {
                        "id": "byName",
                        "options": "Node memory utilization"
                     },
                     "properties": [
                        {
                           "id": "unit",
                           "value": "percent"
                        }
                     ]
                  },
                  {
                     "matcher": {
                        "id": "byName",
                        "options": "Node up"
                     },
                     "properties": [
                        {
                           "id": "unit",
                           "value": "none"
                        }
                     ]
                  },
                  {
                     "matcher": {
                        "id": "byName",
                        "options": "Node up"
                     },
                     "properties": [
                        {
                           "id": "mappings",
                           "value": [
                              {
                                 "options": {
                                    "0": {
                                       "color": "red",
                                       "index": 0,
                                       "text": "Down"
                                    },
                                    "1": {
                                       "color": "green",
                                       "index": 1,
                                       "text": "Up"
                                    }
                                 },
                                 "type": "value"
                              }
                           ]
                        }
                     ]
                  }
               ]
            },
