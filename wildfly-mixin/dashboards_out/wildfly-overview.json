         {
            "datasource": {
               "type": "datasource",
               "uid": "-- Mixed --"
            },
            "description": "Number of sessions that have expired for a deployment over time",
            "fieldConfig": {
               "defaults": {
                  "custom": {
                     "spanNulls": false
