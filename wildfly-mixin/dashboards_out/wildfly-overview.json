            "pluginVersion": "v11.0.0",
            "targets": [
               {
                  "datasource": {
                     "type": "prometheus",
                     "uid": "${prometheus_datasource}"
                  },
                  "expr": "increase(wildfly_undertow_expired_sessions_total{job=~\"$job\",cluster=~\"$cluster\",instance=~\"$instance\",deployment=~\"$deployment\"}[$__interval])",
                  "format": "time_series",
                  "instant": false,
                  "interval": "1m",
                  "intervalFactor": 2,
                  "legendFormat": "{{deployment}}",
                  "refId": "Expired Sessions"
